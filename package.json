{
	"name": "SimpleCMS",
	"version": "0.0.3",
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite dev --open",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"typesafe-i18n": "typesafe-i18n",
		"start": "npm run dev",
		"stop": "npx kill-port 5173"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "0.99.5",
		"@sveltejs/adapter-node": "latest",
		"@sveltejs/kit": "1.3.9",
		"@types/cookie": "0.5.1",
		"@typescript-eslint/eslint-plugin": "5.50.0",
		"@typescript-eslint/parser": "5.50.0",
		"autoprefixer": "10.4.13",
		"eslint": "8.33.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.3",
		"prettier-plugin-svelte": "2.9.0",
		"svelte": "3.55.1",
		"svelte-check": "3.0.3",
		"svelte-preprocess": "5.0.1",
		"tailwindcss": "3.2.4",
		"tslib": "2.5.0",
		"typesafe-i18n": "5.21.0",
		"typescript": "~4.9.5",
		"vite": "4.0.4"
	},
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^3.1.0",
		"@lucia-auth/adapter-mongoose": "0.4.0",
		"@lucia-auth/oauth": "0.5.2",
		"@lucia-auth/sveltekit": "0.6.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/line-clamp": "0.4.2",
		"@tailwindcss/typography": "0.5.9",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/nodemailer": "6.4.7",
		"axios": "^1.3.1",
		"devalue": "4.2.3",
		"ioredis": "5.3.0",
		"jsonwebtoken": "^9.0.0",
		"lucia-auth": "0.6.0",
		"mongoose": "6.9.0",
		"nodemailer": "6.9.1"
	}
}
